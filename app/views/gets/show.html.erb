<p id="notice"><%= notice %></p>

<h2><%= @get.user_id%></h2>
  <%= image_tag @get.img.url(:large) %>

<div class="show actions">
  <%= link_to 'Edit', edit_get_path(@get) %>
  <%= link_to 'Remove', @get, confirm: 'Are you sure you want to lose a friend?', method: :delete, class: 'destroy' %>
  <%= link_to 'Back to all my gets', gets_path %>
</div>